{
  "name": "@workflow/cli",
  "version": "4.0.1-beta.34",
  "description": "Command-line interface for Workflow DevKit",
  "type": "module",
  "bin": {
    "workflow": "./bin/run.js",
    "wf": "./bin/run.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/workflow.git",
    "directory": "packages/cli"
  },
  "oclif": {
    "bin": "workflow",
    "dirname": "workflow",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "devDependencies": {
    "@types/node": "22.19.0",
    "@workflow/tsconfig": "4.0.1-beta.0"
  },
  "dependencies": {
    "@oclif/core": "4.0.0",
    "@oclif/plugin-help": "6.2.31",
    "@swc/core": "1.15.3",
    "boxen": "8.0.1",
    "builtin-modules": "5.0.0",
    "chalk": "5.6.2",
    "chokidar": "4.0.3",
    "comment-json": "4.2.5",
    "date-fns": "4.1.0",
    "easy-table": "1.2.0",
    "enhanced-resolve": "5.18.2",
    "esbuild": "^0.25.11",
    "find-up": "7.0.0",
    "mixpart": "0.0.4",
    "open": "10.2.0",
    "ora": "8.2.0",
    "terminal-link": "5.0.0",
    "tinyglobby": "0.2.14",
    "xdg-app-paths": "5.1.0",
    "zod": "4.1.11",
    "@workflow/builders": "4.0.1-beta.28",
    "@workflow/swc-plugin": "4.0.1-beta.12",
    "@workflow/errors": "4.0.1-beta.10",
    "@workflow/core": "4.0.1-beta.29",
    "@workflow/web": "4.0.1-beta.18",
    "@workflow/world": "4.0.1-beta.10",
    "@workflow/world-local": "4.0.1-beta.19",
    "@workflow/world-vercel": "4.0.1-beta.22"
  },
  "scripts": {
    "build": "tsc && chmod +x bin/run.js",
    "clean": "tsc --build --clean && rm -r dist ||:",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  }
}